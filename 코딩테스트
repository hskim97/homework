<?php
$arr = array(1000, 1, -3000, 9, 0, -10, -1, 2, 4, 10000, -12345);
$arr2 = array();
for ($i = 0 ;$i < count($arr); $i++){
    if($arr[$i] > 0){
        array_push($arr2, $arr[$i]);
    }
}

$minint = min($arr2);

for ($i = 1; $i< max($arr2); $i++){
    if(!in_array($i, $arr2)){
        echo "배열의 매개변수에 없는 가장 작은 양의 정수 : ".$i;
        break;
    }
}
?>

