<body>

<?php
    $arr = array(1, 3, 4, 6);
    $arr2 = array();
    $cnt = count($arr);
    for ($i = 0 ;$i < $cnt; $i++){
        if($arr[$i] > 0){
            array_push($arr2, $arr[$i]);
        }
    }

    echo "정수 : ".min($arr2)+1;

?>

</body>
